// A = 3; B = 5 -> 243 (3‚Åµ)
// A = 2; B = 3 -> 8

int Power(int first, int second)
{
if (second < 1)
{
return 1;
}

return first * Power( first, second - 1);

}

System.Console.WriteLine(Power(3, 4));
