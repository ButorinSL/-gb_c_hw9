bool Simple(int numb, int delitel=0)
{

if (delitel == 0)
{
delitel = numb-1;
}

if (delitel == 1)
{
return true;
}

return numb%delitel !=0 && Simple(numb, delitel-1);

}

System.Console.WriteLine(Simple(43));
